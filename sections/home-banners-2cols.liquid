{% schema %}
{
    "name": "2 Columns Banners",
    "class": "index-section index-section--banners-2cols",
    "settings": [
        {
            "type": "checkbox",
            "id": "has_container",
            "label": "Within container",
            "default": true
        },
        {
            "type": "checkbox",
            "id": "no_gutter",
            "label": "No gutter",
            "default": false
        },
        {
            "type": "header",
            "content": "Spacing (large screen)"
        },
        {
            "type": "range",
            "id": "mt_lg",
            "label": "Margin top",
            "unit": "x",
            "min": 0,
            "max": 20,
            "step": 1,
            "default": 8
        },
        {
            "type": "range",
            "id": "pt_lg",
            "label": "Padding top",
            "unit": "x",
            "min": 0,
            "max": 20,
            "step": 1,
            "default": 0
        },
        {
            "type": "range",
            "id": "mb_lg",
            "label": "Margin bottom",
            "unit": "x",
            "min": 0,
            "max": 20,
            "step": 1,
            "default": 0
        },
        {
            "type": "range",
            "id": "pb_lg",
            "label": "Padding bottom",
            "unit": "x",
            "min": 0,
            "max": 20,
            "step": 1,
            "default": 0
        },
        {
            "type": "header",
            "content": "Spacing (mobile screen)"
        },
        {
            "type": "range",
            "id": "mt",
            "label": "Margin top",
            "unit": "x",
            "min": 0,
            "max": 20,
            "step": 1,
            "default": 5
        },
        {
            "type": "range",
            "id": "pt",
            "label": "Padding top",
            "unit": "x",
            "min": 0,
            "max": 20,
            "step": 1,
            "default": 0
        },
        {
            "type": "range",
            "id": "mb",
            "label": "Margin bottom",
            "unit": "x",
            "min": 0,
            "max": 20,
            "step": 1,
            "default": 0
        },
        {
            "type": "range",
            "id": "pb",
            "label": "Padding bottom",
            "unit": "x",
            "min": 0,
            "max": 20,
            "step": 1,
            "default": 0
        }
    ],
    "blocks": [
        {
            "type": "banner_1",
            "name": "Banner - Type 1",
            "settings": [
                {
                    "type": "header",
                    "content": "Position"
                },
                {
                    "type": "select",
                    "id": "position",
                    "label": "Width on Desktop",
                    "options": [
                        {
                            "value": "col_1",
                            "label": "Left column"
                        },
                        {
                            "value": "col_2",
                            "label": "Right column"
                        },
                        {
                            "value": "bottom",
                            "label": "Bottom"
                        }
                    ],
                    "default": "col_1"
                },
                {
                    "type": "header",
                    "content": "Width"
                },
                {
                    "type": "select",
                    "id": "width_destop",
                    "label": "Width on Desktop",
                    "options": [
                        {
                            "value": "6",
                            "label": "50%"
                        },
                        {
                            "value": "12",
                            "label": "100%"
                        }
                    ],
                    "default": "12"
                },
                {
                    "type": "select",
                    "id": "width_tablet",
                    "label": "Width on Tablet",
                    "options": [
                        {
                            "value": "6",
                            "label": "50%"
                        },
                        {
                            "value": "12",
                            "label": "100%"
                        }
                    ],
                    "default": "12"
                },
                {
                    "type": "header",
                    "content": "Content"
                },
                {
                    "type": "image_picker",
                    "id": "image",
                    "label": "Background image"
                },
                {
                    "type": "text",
                    "id": "title",
                    "label": "Title",
                    "default": "Banner title"
                },
                {
                    "type": "text",
                    "id": "sub_title",
                    "label": "Sub title"
                },
                {
                    "type": "url",
                    "id": "button_url",
                    "label": "Button URL"
                },
                {
                    "type": "header",
                    "content": "Layout"
                },
                {
                    "type": "select",
                    "id": "content_hori_align",
                    "label": "Content horizontal align",
                    "options": [
                        {
                            "value": "x-left pl-8",
                            "label": "Left"
                        },
                        {
                            "value": "w-100 text-center",
                            "label": "Center"
                        },
                        {
                            "value": "x-right pr-8",
                            "label": "Right"
                        }
                    ],
                    "default": "w-100 text-center"
                },
                {
                    "type": "select",
                    "id": "content_vert_align",
                    "label": "Content vertical align",
                    "options": [
                        {
                            "value": "y-top pt-8",
                            "label": "Top"
                        },
                        {
                            "value": "y-50",
                            "label": "Middle"
                        },
                        {
