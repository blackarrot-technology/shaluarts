<section class="banner-mass mt-{{ section.settings.mt}} mb-{{ section.settings.mb}} pt-{{ section.settings.pt}} pb-{{ section.settings.pb}} mt-lg-{{ section.settings.mt_lg}} mb-lg-{{ section.settings.mb_lg}} pt-lg-{{ section.settings.pt_lg}} pb-lg-{{ section.settings.pb_lg}}" id="{{ section.id }}" data-section-id="{{ section.id }}" data-section-type="banners-2cols-section">
    <div class="{% if section.settings.has_container %}container{% endif %}">
        <div class="row {% if section.settings.no_gutter == true %}gutter-no{% else %}gutter-lg{% endif %}">
            <div class="col-lg-6">
                <div class="row {% if section.settings.no_gutter == true %}gutter-no{% else %}gutter-lg{% endif %}">
                    {% for block in section.blocks %}
                    {% if block.settings.position == 'col_1' %}
                    <div class="col-lg-{{ block.settings.width_destop}} col-md-{{ block.settings.width_tablet}} col-12{% unless section.settings.no_gutter == true %} mb-6{% endunless %} appear-animate" data-animation-options="{
                        'name': '{% cycle "fadeInRightShorter", "fadeInLeftShorter" %}',
                        'delay': '.2s'
                    }" {{ block.shopify_atrributes }}>
                        {% case block.type %}
                        {% when 'banner_1' %}
                        <div class="rt-banner banner-fixed banner_1 hover-zoom ani-wrapper-fadeInRight">
                            <a href="{{ block.settings.button_url | default: "javascript:void(0);"}}">
                                {% if block.settings.image != blank %}
                                {% if section.settings.has_container == true %}
                                {% assign _container_width = 1220 %}
                                {% else %}
                                {% assign _container_width = 1820 %}
                                {% endif %}
                                {% case block.settings.width_destop %}
                                    {% when '3' %}
                                    {% assign _img_width = _container_width | divided_by: 4 %}
                                    {% when '4' %}
                                    {% assign _img_width = _container_width | divided_by: 3 %}
                                    {% when '6' %}
                                    {% assign _img_width = _container_width | divided_by: 2 %}
                                    {% when '8' %}
                                    {% assign _img_width = _container_width | divided_by: 3 | plus: 2 %}
                                    {% when '12' %}
                                    {% assign _img_width = _container_width %}
                                {% endcase %}
                                {% assign _img_height = _img_width | divided_by: block.settings.image.aspect_ratio | round %}
                                {% assign _img_url = _img_width | append: 'x' | append: _img_height %}
                                <figure>
                                    <img src="{{ block.settings.image | img_url: _img_url, scale: 2 }}" alt="banner" width="{{ _img_width }}" height="{{ _img_height }}"/>
                                </figure>
                                {% else %}
                                <figure style="background-color:#ccc;height:240px"></figure>
                                {% endif %}
                            </a>
                            <div class="banner-content {{ block.settings.content_hori_align }} {{ block.settings.content_vert_align }} appear-animate pe-none">
                                {% if block.settings.sub_title %}
                                <h4 class="banner-subtitle text-{{ block.settings.color_text }} font-weight-light fs-{{ block.settings.fs_subtitle | replace: '.', 'p' }} mb-2">{{ block.settings.sub_title }}</h4>
                                {% endif %}
                                {% if block.settings.title %}
                                <h3 class="banner-title ls-m mb-1 text-{{ block.settings.color_text }} font-weight-normal fs-{{ block.settings.fs_title | replace: '.', 'p' }}">{{ block.settings.title }}</h3>
                                {% endif %}
                                {% if block.settings.button_text != blank %}
                                <a href="{{ block.settings.button_url | default: "javascript:void(0);"}}" class="btn btn-dark btn-md">
                                    {{block.settings.button_text}}
                                </a>
                                {% else %}
                                <a href="{{ block.settings.button_url | default: "javascript:void(0);"}}" class="text-{{ block.settings.color_text }} btn-arrow ani-el">
                                    {% render 'icon-arrow-right' %}
                                </a>
                                {% endif %}
                            </div>
                        </div>
                        {% when 'banner_2' %}
                        <div class="rt-banner banner-fixed banner_2 content-{{ block.settings.content_hori_align }} hover-zoom hover-gloom appear-animate">
                            {% if block.settings.image != blank %}
                            <figure class="d-flex">
                                <img src="{{ block.settings.image | img_url: 'master' }}" alt="banner" />
                            </figure>
                            {% else %}
                            <figure style="min-height:400px;background-color:#ccc;"></figure>
                            {% endif %}
                            <div class="banner-content {{ block.settings.content_vert_align }}">
                                <div class="appear-animate" data-animation-options="{
                                    'name': 'fadeInUpShorter',
                                    'delay': '.2s'
                                }">
                                    <h3 class="banner-title font-weight-slight-bold ls-m text-{{ block.settings.color_text }} fs-{{ block.settings.fs_title | replace: '.', 'p' }}">{{ block.settings.title }}</h3>
                                    <p class="text-{{ block.settings.color_text }} fs-{{ block.settings.fs_description | replace: '.', 'p' }}">{{ block.settings.description }}</p>
                                    {% if block.settings.button_text != blank %}
                                    <a href="{{ block.settings.button_url | default: 'javascript:void(0);' }}" class="btn btn-primary btn-shadow-lg btn-ellipse text-normal">{{ block.settings.button_text }}</a>
                                    {% endif %}
                                </div>
                            </div>
                        </div>
                        {% when 'banner_3' %}
                        <div class="rt-banner hover-zoom hover-gloom appear-animate" style="background-color:{{ block.settings.color_bg }}">
                            {% if block.settings.image != blank %}
                            <figure class="d-flex">
                                <img src="{{ block.settings.image | img_url: 'master' }}" alt="banner" />
                            </figure>
                            {% endif %}
                            <div class="banner-content text-center mt-5 mb-5">
                                <a href="{{ block.settings.button_url | default: "javascript:void(0);"}}">
                                    <div class="appear-animate" data-animation-options="{
                                        'name': 'fadeInUpShorter',
                                        'delay': '.2s'
                                    }">
                                        <div class="d-flex flex-wrap justify-content-center align-items-center">
                                            <h3 class="banner-title font-weight-slight-bold ls-m text-{{ block.settings.color_text }} fs-{{ block.settings.fs_title | replace: '.', 'p' }}">{{ block.settings.title }}</h3>
                                            {% if block.settings.button_text != blank %}
                                            <span class="text-{{ block.settings.color_text }} text-normal fs-1p3 ml-lg-5 pl-3 pr-3 pt-1 pb-1" style="border: 1px dashed">{{ block.settings.button_text }}</span>
                                            {% endif %}
                                        </div>
                                        <p class="text-{{ block.settings.color_text }} fs-{{ block.settings.fs_description | replace: '.', 'p' }} mb-0">{{ block.settings.description }}</p>
                                    </div>
                                </a>
                            </div>
                        </div>
                        {% endcase %}
                    </div>
                    {% endif %}
                    {% endfor %}
                </div>
            </div>
